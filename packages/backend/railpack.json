{
  "$schema": "https://schema.railpack.com",
  "language": "python",
  "install": {
    "command": "poetry install --no-interaction --no-ansi --only main --no-root"
  },
  "build": {
    "command": "python -m alembic upgrade head || true"
  },
  "start": {
    "command": "python -m uvicorn main:app --host 0.0.0.0 --port $PORT"
  }
}

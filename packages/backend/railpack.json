{
  "$schema": "https://schema.railpack.com",
  "language": "python",
  "build": {
    "commands": [
      "python -V",
      "pip install --upgrade pip",
      "pip install poetry",
      "poetry config virtualenvs.create false",
      "poetry install --no-dev --no-interaction"
    ]
  },
  "start": {
    "command": "poetry run python -m experiments.web.dashboard_server"
  }
}
